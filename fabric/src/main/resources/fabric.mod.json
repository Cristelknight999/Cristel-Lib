{
  "schemaVersion": 1,
  "id": "cristellib",
  "version": "${version}",
  "name": "Cristel Lib",
  "description": "A Library mod for easy structure config and runtime datapacks.",
  "authors": [
    "Cristelknight999"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC BY-NC-ND 4.0",
  "icon": "assets/cristellib/textures/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.cristellib.fabric.CristelLibFabric"
    ],
    "cristellib": [
      "net.cristellib.api.BuiltInAPI"
    ],
    "preLaunch": [
      "net.cristellib.fabriclike.CristelLibPreInit"
    ]
  },
  "mixins": [
    "cristellib.mixins.json",
    "cristellib-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "~1.19.3"
