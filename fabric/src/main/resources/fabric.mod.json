{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Cristelknight"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}"
  },
  "license": "${mod_license}",
  "icon": "pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.cristelknight.cristellib.fabric.CristelLibFabric"
    ],
    "cristellib": [
      "de.cristelknight.cristellib.api.BuiltInAPI"
    ]
  },
  "mixins": [
    "cristellib.mixins.json",
    "cristellib-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_min_version}",
    "fabric": ">=${fabric_api_version}",
    "minecraft": ">=${minecraft_version}"
  }
}